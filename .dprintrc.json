{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "projectType": "openSource",
  "incremental": true,
  "typescript": {
    "indentWidth": 2
  },
  "json": {
    "indentWidth": 2
  },
  "includes": ["**/*.{ts,tsx,js,jsx,json}"],
  "excludes": [
    "**/public"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.40.3.wasm",
    "https://plugins.dprint.dev/json-0.8.0.wasm"
  ]
}
